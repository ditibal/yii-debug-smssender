{
    "name": "ditibal/yii2-smssender",
    "description": "Extensions for sending sms via different services",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "sms",
        "sms-sender"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andrew Sergeev",
            "email": "ditibal@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-httpclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "ditibal\\smssender\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
